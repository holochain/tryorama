{
  "name": "@holochain/tryorama",
  "description": "Toolset to manage Holochain conductors and facilitate running test scenarios",
  "version": "0.18.1",
  "author": "Holochain Foundation",
  "license": "MIT",
  "keywords": [
    "holochain",
    "conductor",
    "test",
    "e2e"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/holochain/tryorama"
  },
  "type": "module",
  "engines": {
    "node": ">=18.0.0 || >=20.0.0"
  },
  "exports": {
    ".": "./lib/index.js"
  },
  "types": "./lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rimraf ./lib && tsc && npm run build:docs",
    "build:docs": "api-extractor run --local && api-documenter markdown -i docs/temp -o docs",
    "prepare": "npm run lint",
    "lint": "eslint --fix --ext .ts ts/src ts/test eslint.config.mjs",
    "format": "prettier --write \"ts/**/*.ts\"",
    "test": "tsx --test ts/test/index.ts",
    "test:conductor": "tsx ts/test/conductor.ts",
    "test:scenario": "tsx ts/test/scenario.ts",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@holochain/client": "^0.19.0",
    "get-port": "^7.0.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "uuid": "^11.0.0",
    "winston": "^3.8.2",
    "ws": "^8.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@microsoft/api-documenter": "^7.19.27",
    "@microsoft/api-extractor": "^7.33.7",
    "@msgpack/msgpack": "^3.1.1",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.14.191",
    "@types/ramda": "^0.30.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-tsdoc": "^0.4.0",
    "prettier": "^3.0.0",
    "rimraf": "^6.0.0",
    "tsx": "^4.7.2",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.0.0"
  }
}
